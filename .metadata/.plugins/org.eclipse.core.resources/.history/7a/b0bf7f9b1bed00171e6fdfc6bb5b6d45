$('document').ready(function() {
	});

function addNote(){
	var note={};
	note.title= $('#title').val();
	note.description = $('#description').val();
	
	$.ajax({
		type:'POST',
		url:'addNote/1',
		data: {
			'title' : note.title,
			'description' : note.description
		},
	success:function(result){
		$('#myModal').modal('hide'),
		addNote();
		console.log(result);
	}
	});
	
	function deleteNote(id) {
	    console.log(id);
	   
	    
	    $.ajax({
	    	 type : 'DELETE',  
	         url : "delete"
		});
	    
	}
}